#include <iostream>
#include <vector>
using namespace std;

template<typename T>
class matrix {
private:
	vector<vector<T> > data;
	T columns;
	T rows;
public:
	matrix(T x, T y) :
			columns(x), rows(y) {
		data.resize(x);
		for (int i = 0; i < y; i++) {
			data[i].resize(y);
		}
	}
};

int main() {


}
